{
  "name": "@gradian/arcviewer",
  "version": "1.4.0",
  "description": "A utility for extracting data given an Algorand asset ID of an ARC3, ARC19, or ARC69 NFT asset for purposes like rendering an NFT in a frontend.",
  "scripts": {
    "test": "jest",
    "build": "rollup --config rollup.config.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Helium-Labs/arcviewer"
  },
  "author": "",
  "license": "MIT",
  "main": "./dist/bundle.cjs",
  "module": "./dist/bundle.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/bundle.mjs"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/bundle.cjs"
      }
    }
  },
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "@json-rpc-tools/utils": "^1.7.6",
    "algosdk": "^2.7.0",
    "axios": "^1.7.7",
    "multiformats": "^9.7.1",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "algosdk": "^2.7.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/browserify": "^12.0.37",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.11.18",
    "algokit-testkit": "^1.2.1",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "rimraf": "^4.4.0",
    "rollup": "^4.21.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.5"
  }
